    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "aws-amplify": "^1.1.22",
    "aws-amplify-react": "^2.3.3-beta.5",
    "bootstrap": "^4.3.1",
