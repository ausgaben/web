    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "aws-amplify": "^3.0.9",
    "aws-amplify-react": "^4.1.8",
    "aws-appsync": "^3.0.4",
    "aws-appsync-auth-link": "^2.0.2",
